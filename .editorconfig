# <ttps://editorconfig.org>

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size=4

[*.css]
indent_style = space
indent_size = 2

[*.js]
indent_style = tab
indent_size = 4

[{package.json,.travis.yml,*.{yaml,yml}}]
indent_style = space
indent_size = 4
